{
  "project_name": "Project Name",
  "repo_name": "{{ cookiecutter.project_name.strip().replace(' ', '-').replace('_', '-').lower() }}",
  "github_username": "",
  "author_name": "{{ cookiecutter.github_username }}",
  "author_email": "{{ cookiecutter.github_username }}@github.com",
  "description": "A short description of the project.",
  "compatible_python_versions": ["3.9", "3.10", "3.11", "3.12"],
  "mkdocs": ["y", "n"],
  "codecov": ["y", "n"],
  "init_git": true,
  "license": [
    "MIT License",
    "BSD License",
    "ISC License",
    "GNU General Public License v3",
    "Apache Software License 2.0",
    "Not open source"
  ],
  "__prompts__": {
      "project_name": " e.g. My awesome data science project",
      "repo_name": "Repository name (e.g. my-awesome-data-science-project)",
      "github_username": "GitHub username",
      "author_name": "Author full name (or your organization/company/team)",
      "author_email": "Your email address or personal website (eq. you@example.com or https://example.com)",
      "mkdocs": {
                  "__prompt__": "Automatic docs with mkdocs?",
                  "y": "yes",
                  "n": "no"
                },
      "codecov": {
                  "__prompt__": "pytest coverage report with codecov?",
                  "y": "yes",
                  "n": "no"
                },
      "init_git": "Initialize a git repository"
  },
  "_copy_without_render": [
    ".code_quality/*",
    ".vscode/*",
    "config/*",
    "data/*",
    "src/*",
    "tests/*",
    ".editorconfig",
    ".pre-commit-config.yaml",
    "codecov.yaml",
    "docs/*",
    "Makefile"
  ]
}
